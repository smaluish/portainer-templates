[
  {
    "type": "container",
    "title": "Homarr",
    "description": "Homarr - A simple and elegant dashboard for your server apps",
    "image": "ghcr.io/ajnart/homarr:latest",
    "ports": [
      "7575:7575"
    ],
    "volumes": [
      {
        "container": "/app/data/configs",
        "bind": "/home/smaluish/docker/homarr/configs"
      },
      {
        "container": "/app/data/icons",
        "bind": "/home/smaluish/docker/homarr/icons"
      },
      {
        "container": "/app/public/icons",
        "bind": "/home/smaluish/docker/homarr/public-icons"
      }
    ],
    "env": [
      {
        "name": "PUID",
        "default": "1000"
      },
      {
        "name": "PGID",
        "default": "1000"
      },
      {
        "name": "TZ",
        "default": "Australia/Perth"
      }
    ],
    "restart_policy": "unless-stopped",
    "logo": "https://raw.githubusercontent.com/Qballjos/portainer_templates/master/Logos/homarr.png"
  }
]

